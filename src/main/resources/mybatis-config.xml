<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
  
  <settings>
      <setting name="mapUnderscoreToCamelCase" value="true"/>
  </settings>

  <typeAliases>
  
    <!-- java.util -->
    <typeAlias type="java.util.Map" alias="Map" />
    <typeAlias type="java.util.HashMap" alias="HashMap" />
    <typeAlias type="java.util.List" alias="List" />
    
    <package name="com.vibesync.board.domain"/>
    <package name="com.vibesync.category.domain"/>
    <package name="com.vibesync.follow.domain"/>
    <package name="com.vibesync.member.domain"/>
    <package name="com.vibesync.note.domain"/>
    
  </typeAliases>

</configuration>
 